<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/auth_nav_graph"
    app:startDestination="@id/loginFragment">
    <fragment
        android:id="@+id/loginFragment"
        android:name="com.route.chatapp.ui.auth.fragments.login.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login">
        <action
            android:id="@+id/action_loginFragment_to_regiisterFragment"
            app:destination="@id/regiisterFragment" />
    </fragment>
    <fragment
        android:id="@+id/regiisterFragment"
        android:name="com.route.chatapp.ui.auth.fragments.register.RegisterFragment"
        android:label="fragment_regiister"
        tools:layout="@layout/fragment_register" />
</navigation>